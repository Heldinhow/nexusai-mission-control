# API Configuration
VITE_ORCHESTRATOR_API_URL=https://orchestrator-api.example.com/v1
VITE_ORCHESTRATOR_WS_URL=wss://orchestrator-api.example.com/v1/stream
VITE_COST_TRACKER_API_URL=https://cost-tracker-api.example.com/v1

# Authentication (if not handled by external auth provider)
VITE_AUTH_TOKEN=your-jwt-token-here

# Feature Flags
VITE_ENABLE_KILL_SWITCH=true
VITE_ENABLE_COST_TRACKING=true
VITE_ENABLE_GRAPH_VISUALIZATION=true

# Performance
VITE_WEBSOCKET_RECONNECT_INTERVAL=5000
VITE_POLLING_INTERVAL=5000
VITE_MAX_GRAPH_DEPTH=5

# Detection Thresholds
VITE_SUSPICIOUS_TRANSITION_THRESHOLD=10
VITE_SUSPICIOUS_TOKEN_MULTIPLIER=5
VITE_SUSPICIOUS_SPAWNING_THRESHOLD=20
VITE_STUCK_IN_PENDING_MINUTES=5

# Development
VITE_DEV_MODE=false
VITE_MOCK_API=false
